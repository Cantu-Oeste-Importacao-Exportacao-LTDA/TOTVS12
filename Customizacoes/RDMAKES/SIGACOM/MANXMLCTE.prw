										{ 'BR_LARANJA'  , "Pré-nota gerada"	          },;
										{ 'BR_VERMELHO'	, "Erro de processamento"  		},;
										{ 'BR_AMARELO'  , "Verificar depois"          },;
										{ 'BR_CINZA'    , "Ct-e Cancelado"            },;
										{ 'BR_AZUL'     , "Evento Executado"          },;
										{ 'BR_VERDE'    , "Pré-nota classificada"     }}											
												
