*-------------------------------*
Static Function fClear(cBuffer)
*-------------------------------*

Local cRet := ""
Local nX   := 0    
Local cCar := ""

For nX := 1 to Len(cBuffer)
	cCar := SubStr(cBuffer,nX,01)
	If IsAlpha(cCar) .or. isDigit(cCar) .or. cCar$(' ()><#.-:=$?+"/')
		cRet += cCar
	EndIf
Next nX

Return cRet 