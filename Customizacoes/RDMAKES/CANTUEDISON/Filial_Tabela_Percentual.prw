Static Function ZTPGRV()

Local aCols		:= oBrw1:aCols
Local aHeader	:= oBrw1:aHeader

Do Case
	//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
	//Executa regras para a inclus„o do cadastro									≥
	//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
	Case INCLUI
		
		//Controle de transaÁ„o
		BEGIN TRANSACTION
			dbSelectArea("ZTP")
			For nI := 1 to Len(aCols)
				If !aCols[nI][Len(aHeader) + 1]
					RecLock("ZTP", .T.)
						//Passa campo a campo do aHeader para verificar se ele È real
						For nY := 1 to Len(aHeader)
							//Verifica se o campo nY È diferente de virtual, no caso se o campo È real
							If aHeader[nY][10] != "V"
								If !EMPTY(nPosZTP := FieldPos(aHeader[nY][2]))
									ZTP->(FieldPut(nPosZTP, aCols[nI][nY]))
								EndIf
							EndIf
						Next nY
						ZTP->ZTP_FILIAL	:= xFilial("ZTP")
						ZTP->ZTP_TABELA := cCod							
						
					ZTP->(MSUNLOCK())
				EndIf
			Next nI	
		
		END TRANSACTION
	
	
	//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
	//Executa regras para a alteraÁ„o do cadastro									≥
	//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹		
	Case ALTERA
	
		BEGIN TRANSACTION
		
			//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
			//Realiza a exclus„o de todos os registros existentes atualmente				≥
			//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
			dbSelectArea("ZTP")
			ZTP->(dbSetOrder(1)) // FILIAL + cod tabela
			ZTP->(dbGoTop())
			If ZTP->(dbSeek(xFilial("ZTP") +  cCod))
			    While ZTP->(!EOF()) .And. ZTP->ZTP_FILIAL == xFilial("ZTP") .And. ZTP->ZTP_TABELA == cCod
				
							
				//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
				//Efetua ‡ exclus„o do registro posicionado										≥
				//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
				RecLock("ZTP", .F.)
					ZTP->(DBDELETE())
				ZTP->(MSUNLOCK())
			
				ZTP->(dbSkip())
				End
			EndIf		
			
			dbSelectArea("ZTP")
			For nI := 1 to Len(aCols)
				If !aCols[nI][Len(aHeader) + 1]
					RecLock("ZTP", .T.)
						//Passa campo a campo do aHeader para verificar se ele È real
						For nY := 1 to Len(aHeader)
							//Verifica se o campo nY È diferente de virtual, no caso se o campo È real
							If aHeader[nY][10] != "V"
								If !EMPTY(nPosZTP := FieldPos(aHeader[nY][2]))
									ZTP->(FieldPut(nPosZTP, aCols[nI][nY]))
								EndIf
							EndIf
						Next nY
						
						ZTP->ZTP_FILIAL	:= xFilial("ZTP")
						ZTP->ZTP_TABELA := cCod
													
					ZTP->(MSUNLOCK())
				EndIf
			Next nI	
					
		END TRANSACTION

	EndCase

Return 