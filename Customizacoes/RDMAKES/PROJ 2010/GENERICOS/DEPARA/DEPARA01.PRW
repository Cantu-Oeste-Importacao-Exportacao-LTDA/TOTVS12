*-----------------------------------------*
Static Function fGetEMP(cEMP, aDADOSIMP)       
*-----------------------------------------*
Local cAliasRC := GetNextAlias()
               
	cQuery := "	SELECT TABLE_NAME 		  						" + chr(13)+chr(10)
	cQuery += "	FROM ALL_TABLES									" + chr(13)+chr(10)
	cQuery += "	WHERE OWNER = 'TOTVS10'							" + chr(13)+chr(10)
	cQuery += "	AND SUBSTR(TABLE_NAME,1,2) = 'RC' 				" + chr(13)+chr(10)
	cQuery += "	AND SUBSTR(TABLE_NAME,3,2) = '"+cEMP+"' 		" + chr(13)+chr(10)
	cQuery += "	AND LENGTH(TABLE_NAME) > 6						" + chr(13)+chr(10)
	
	TCQUERY ChangeQuery( cQuery ) NEW ALIAS (cAliasRC)
	
	dbSelectArea(cAliasRC)
	(cAliasRC)->(dbGoTop())
	While (cAliasRC)->(!EOF())		
		aAdd ( aDADOSIMP, { "GPE", ALLTRIM((cAliasRC)->TABLE_NAME), "RC_PD"	, .F., cEMP	} )
		(cAliasRC)->(dbSkip())
	Enddo
	(cAliasRC)->(dbCloseArea())

	 
Return
                        

