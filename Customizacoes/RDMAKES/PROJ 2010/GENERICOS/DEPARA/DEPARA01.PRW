/*/
*------------------------------------------------------*
Static Function fRepVERB( cCODEMP, cCODMOD, cPDVERBAS )
*------------------------------------------------------*
Local cRET := STRING_NULL

	If Len(cPDVERBAS) > 0            
		cAux := STRING_NULL
		For nX := 1 to Len(cPDVERBAS)			
			If SubSTR(cPDVERBAS,nX,1) != ","
				cAux += SubSTR(cPDVERBAS,nX,1)
			ElseIf cAux != STRING_NULL 
				nPOSVRB := aScan ( aDADOSVRB, {|x| x[03] == cCODEMP .and. x[01] == cAux .and. x[04] == cCODMOD } )
				If nPOSVRB != 0
					cRET += aDADOSVRB[nPOSVRB][02]+","
				Else
	    			cTXT := StrTran(time(),":","")+"-2-ERRO: VERBA N√O ENCONTRADA: "+cAux+chr(13)+chr(10)
					fWrite(nHdlLOG,cTXT,Len(cTXT))					
				EndIf
				cAux := STRING_NULL
			EndIf
		Next nX
	EndIf
	
Return (cRET)

