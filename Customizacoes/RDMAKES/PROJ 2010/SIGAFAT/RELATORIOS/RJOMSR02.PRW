*---------------------------------------------------------------------*
Static Function RunReport(Cabec1,Cabec2,Titulo,nLin,nOrdem)
*---------------------------------------------------------------------*
Local nQTDCLI   := 0
Local cQuery    := STRING_NULL
Local cAliasTMP := GetNextAlias()

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Filtro de registros                                                 ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤ
	
	
	cQuery := "	SELECT													" + chr(13)
	cQuery += "			ZZ5.ZZ5_FILIAL,									" + chr(13)
	cQuery += "			ZZ5.ZZ5_CLIENT,									" + chr(13)
	cQuery += "			ZZ5.ZZ5_LOJA,  									" + chr(13)
	cQuery += "			ZZ5.ZZ5_VEND  									" + chr(13)
	cQuery += "	FROM " + RetSQLName("ZZ5") + " ZZ5		   				" + chr(13)
	cQuery += "	WHERE 									   				" + chr(13)
	cQuery += "			ZZ5.ZZ5_FILIAL BETWEEN							" + chr(13)
	cQuery += "			'" + mv_par01 + "' AND '" + mv_par02 + "'		" + chr(13)
	cQuery += "	AND		ZZ5.ZZ5_VEND BETWEEN							" + chr(13)
	cQuery += "			'" + mv_par03 + "' AND '" + mv_par04 + "'		" + chr(13)
	cQuery += "	AND		ZZ5.D_E_L_E_T_ != '*'							" + chr(13)
	cQuery += "	ORDER BY												" + chr(13)
	cQuery += "			ZZ5.ZZ5_VEND,  									" + chr(13)
	cQuery += "			ZZ5.ZZ5_FILIAL,									" + chr(13)
	cQuery += "			ZZ5.ZZ5_CLIENT,									" + chr(13)
	cQuery += "			ZZ5.ZZ5_LOJA  									" + chr(13)
	
	memowrite("RJOMSR02.SQL",cQuery)
	
	TCQUERY ChangeQuery( cQuery ) NEW ALIAS (cAliasTMP)
	
	dbSelectArea(cAliasTMP)
	(cAliasTMP)->(dbGoTop())
	SetRegua((cAliasTMP)->(RecCount()))
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Percorre itens selecionados para impressใo                          ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	
	cCODVEND := STRING_NULL
	
	While (cAliasTMP)->(!EOF())

		//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
		//ณ Verifica o cancelamento pelo usuario...                             ณ
		//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	
		If lAbortPrint
			@nLin,00 PSAY "*** CANCELADO PELO OPERADOR ***"
			Exit
		Endif
	
		//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
		//ณ Impressao do cabecalho do relatorio. . .                            ณ
		//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	
		If nLin > 65 .OR. cCODVEND != (cAliasTMP)->ZZ5_VEND
			
			If cCODVEND != (cAliasTMP)->ZZ5_VEND .and. cCODVEND != STRING_NULL
				@nLin,000 PSAY "NฺMERO TOTAL DE CLIENTES ---> " + cValToChar(nQTDCLI)
				nQTDCLI := 0
			EndIf
			
			cCabec2 := Cabec2 + (cAliasTMP)->ZZ5_VEND + "/" + POSICIONE("SA3",1,xFilial("SA3")+(cAliasTMP)->ZZ5_VEND,"A3_NOME")
			Cabec(Titulo,Cabec1,cCabec2,NomeProg,Tamanho,nTipo)			
			nLin := 9                                          
			
		Endif
		
		nQTDCLI += 1		
		cCODVEND := (cAliasTMP)->ZZ5_VEND

		//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
		//ณ Impressใo dos dados                                                 ณ
		//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤ
	    
		dbSelectArea("SA1")
		dbSetOrder(1)
		dbSeek ( xFilial("SA1") + (cAliasTMP)->ZZ5_CLIENT + (cAliasTMP)->ZZ5_LOJA )
		
		cDESCOND := " "
		If !Empty (SA1->A1_COND)
			dbSelectArea("SE4")
			dbSetOrder(1)
			dbSeek( xFilial("SE4") + SA1->A1_COND )
			cDESCOND := SA1->A1_COND + " - " + SE4->E4_DESCRI
		EndIf		
	
		@nLin,000 PSAY (cAliasTMP)->ZZ5_FILIAL
		@nLin,008 PSAY (cAliasTMP)->ZZ5_CLIENT + "/" + (cAliasTMP)->ZZ5_LOJA
		@nLin,024 PSAY SA1->A1_NOME
		@nLin,065 PSAY SA1->A1_CGC 
		@nLin,081 PSAY SA1->A1_DDD + SA1->A1_TEL           
		@nLin,095 PSAY SubSTR(cDESCOND,1,28)
		@nLin,120 PSAY TRANSFORM(SA1->A1_LC, "@E 999,999,999.99")
		nLin += 1
				
		IncRegua()
		(cAliasTMP)->(dbSkip())
	   
	EndDo
	
	(cAliasTMP)->(dbCloseArea())

	If cCODVEND != STRING_NULL
		@nLin,000 PSAY "NฺMERO TOTAL DE CLIENTES ---> " + cValToChar(nQTDCLI)
	EndIf


	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Finaliza a execucao do relatorio...                                 ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	
	SET DEVICE TO SCREEN
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Se impressao em disco, chama o gerenciador de impressao...          ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	
	If aReturn[5]==1
	   dbCommitAll()
	   SET PRINTER TO
	   OurSpool(wnrel)
	Endif
	
	MS_FLUSH()

Return



