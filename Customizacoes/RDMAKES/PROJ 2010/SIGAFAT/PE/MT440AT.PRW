User Function MT440AT()     
                     
Local lRet := .T. 
Local aAreaTMP := GetArea()         

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ
//³Chama função para monitor uso de fontes customizados³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ
U_USORWMAKE(ProcName(),FunName())

	If (SC5->(FieldPos("C5_X_IB2B")) > 0)
		If SC5->C5_X_IB2B == "S"
			If ! ALLTRIM(UPPER(cUserName)) $ ALLTRIM(UPPER(SuperGetMV("MV_X_LEB2B",,"")))				
				Aviso("Aviso","Pedido B2B somente poderá ser liberado pelos responsáveis do B2B.",{"OK"})
				lRet := .F.
			EndIf
		EndIf	
	EndIf

	RestArea(aAreaTMP)

Return(lRet)