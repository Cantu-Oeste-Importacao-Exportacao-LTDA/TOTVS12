User Function RJFP14TdOk()
*------------------------------*  
Local lRet 		:= .T.
Local nPDEP 	:= aScan(aHeader,{|x| AllTrim(x[2])=="Z65_CODDEP"})
Local nPPAI 	:= aScan(aHeader,{|x| AllTrim(x[2])=="Z65_DEPPAI"})
Local nMaxFor   := Len(aCols)
Local nUsado    := Len(aHeader)
Local lTestaDel := nUsado <> Len(aCols[1])
Local lDefPai	:= .F.

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤ
//ณChama fun็ใo para monitor uso de fontes customizadosณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤ
U_USORWMAKE(ProcName(),FunName())   
	
	For nI := 1 to Len(aCols)              
		// verifica se existe registro duplicado
		If ! aCols[nI][Len(aCols[nI])]			
			For nJ := 1 to Len(aCols)			
				If nJ != nI 
					If aCols[nI,nPDEP] == aCols[nJ,nPDEP] .and. ! aCols[nJ][Len(aCols[nJ])]	
						MsgAlert("O c๓digo do departamento "+ ALLTRIM(aCols[nI,nPDEP]) +" estแ duplicado!")
						lRet := .F.
						Exit
					EndIf
					If aCols[nI,nPPAI] == aCols[nJ,nPPAI] .and. ! aCols[nJ][Len(aCols[nJ])] .and. aCols[nJ,nPPAI] == "S"	
						MsgAlert("Somente um departamento poderแ ser definido como pai!")
						lRet := .F.
						Exit
					EndIf
				EndIf
			Next nJ
			If aCols[nI,nPPAI] == "S"	
				lDefPai := .T.
			EndIf
		EndIf                         		
	Next nI	           
	
	
	If !lDefPai
		MsgAlert("Um departamento deverแ ser definido como pai!")
		lRet := .F.
	EndIf

Return (lRet)


