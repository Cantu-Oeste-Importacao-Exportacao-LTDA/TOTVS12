Static Function fReplDEP()

Local nREG := 0
Local aDEP := {}

	dbSelectArea("Z65")
	dbSetOrder(1)
	If dbSeek(xFilial("Z65")+mv_par01)
		While !Z65->(EOF()) .and. Z65->(Z65_FILIAL+Z65_CODPRD) == xFilial("Z65")+mv_par01
			aAdd (aDEP, {Z65->Z65_CODDEP, Z65->Z65_DESDEP, Z65->Z65_DEPPAI })
			Z65->(dbSkip())
		Enddo		
	EndIf    
	
	If Len(aDEP) == 0
		Aviso("Aviso","Nใo foram localizados departamentos para o produto base.",{"OK"})
		return
	EndIf

	dbSelectArea("SB1")
	dbSetOrder(1)
	If dbSeek(xFilial("SB1")+mv_par02)
		While !SB1->(EOF()) .and. SB1->B1_FILIAL == xFilial("SB1") .and. SB1->B1_COD >= mv_par02 .and. SB1->B1_COD <= mv_par03

			dbSelectArea("Z65")
			dbSetOrder(1)
			If !dbSeek(xFilial("Z65")+SB1->B1_COD)
				For nI := 1 to Len(aDEP)
					If RecLock("Z65",.T.)
						Z65->Z65_FILIAL	:= xFilial("Z65")
						Z65->Z65_CODPRD	:= SB1->B1_COD
						Z65->Z65_DESPRD	:= SB1->B1_DESC
						Z65->Z65_CODDEP	:= aDEP[nI,01]
						Z65->Z65_DESDEP	:= aDEP[nI,02]
						Z65->Z65_DEPPAI	:= aDEP[nI,03]
						Z65->Z65_AB2B	:= "S"
						Z65->(MsUnLock())
					EndIf
				Next nI         
				nREG += 1
			EndIf
		
			SB1->(dbSkip())		
		Enddo           
	EndIf

	Aviso("Aviso","Departamento(s) incluํdo(s) para "+cValToChar(nREG)+" produto(s)",{"OK"})
	
Return


