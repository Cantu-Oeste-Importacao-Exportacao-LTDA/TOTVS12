Static Function RJFP14GRV( cEntidade, aRecZ65, lExclui )

Local cSeekZ65  := ""
Local lGravou   := .F.
Local nLoop     := 0
Local nLoop2    := 0

DEFAULT lExclui := .F.

If lExclui

	cSeekZ65 := xFilial( cEntidade ) + M->cSB1_COD
	dbSelectArea(cEntidade)
	(cEntidade)->( dbSetOrder(1) )
	If (cEntidade)->( dbSeek( cSeekZ65 ) )
		lGravou := .T.
		While !(cEntidade)->( EOF() ) .And. cSeekZ65 == Z65->Z65_FILIAL + Z65->Z65_CODPRD
			RecLock( cEntidade, .F. )
			(cEntidade)->Z65_AB2B := "S"
			(cEntidade)->( dbDelete() )
			(cEntidade)->( MsUnLock() ) 			
			(cEntidade)->( dbSkip() )
		EndDo 	
	EndIf 	                  
	
Else

	For nLoop := 1 To Len( aCols )
		lGravou := .T.
		If GDDeleted( nLoop )
			If nLoop <= Len( aRecZ65 )
				(cEntidade)->( MsGoto( aRecZ65[ nLoop ] ) ) 	
				RecLock( cEntidade, .F. )
				(cEntidade)->Z65_AB2B := "S"
				(cEntidade)->( dbDelete() )
				(cEntidade)->( MsUnlock() ) 		
			EndIf
		Else
			If nLoop <= Len( aRecZ65 )
				(cEntidade)->( MsGoto( aRecZ65[ nLoop ] ) ) 	
				RecLock( cEntidade, .F. )
			Else
				//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
				//ณ Inclui e grava os campos chave                                         ณ
				//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
				RecLock( cEntidade, .T. ) 		
				Z65->Z65_FILIAL	:= xFilial( cEntidade )
				Z65->Z65_CODPRD	:= M->cSB1_COD
				Z65->Z65_DESPRD	:= M->cSB1_DESC
				
			EndIf
			
			//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
			//ณ Grava os demais campos                                                 ณ
			//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
			For nLoop2 := 1 To Len( aHeader )
				If ( aHeader[nLoop2,10] <> "V" ) .And. !( AllTrim( aHeader[nLoop2,2] ) $ "Z65_FILIAL|Z65_CODPRD|Z65_DESPRD" )
					Z65->(FieldPut(FieldPos(aHeader[nLoop2,2]),aCols[nLoop,nLoop2]))
				EndIf
			Next nLoop2

			Z65->Z65_AB2B := "S"
			Z65->( MsUnlock() )

		EndIf

	Next nLoop

EndIf 	

Return( lGravou )

