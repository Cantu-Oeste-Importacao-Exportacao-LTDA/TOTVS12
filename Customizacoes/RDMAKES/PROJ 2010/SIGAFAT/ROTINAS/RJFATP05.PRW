*-------------------------*
User Function RJFP5VCV()
*-------------------------*
Local aAreaTMP := GetArea()
Local nPOSPROD := aScan( aHeader, {|x| AllTrim(Upper(x[2])) == "C6_PRODUTO" })
Local lRet := .T.                

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ
//³Chama função para monitor uso de fontes customizados³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ
U_USORWMAKE(ProcName(),FunName())

	For nI := 1 to Len(aCols)
		If !Empty(aCols[nI][nPOSPROD])
			Aviso("Aviso","Não é possível informar um valor para este campo após ter informado código do produto no pedido!", {"OK"}, 2)			
			lRet := .F.                                                                                                           
			Exit
		EndIf
	Next nI
			
	RestArea(aAreaTMP)

Return (lRet)

