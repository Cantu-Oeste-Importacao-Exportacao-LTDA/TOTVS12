/* NÃO É MAIS UTILIZADA ESSA AMARRAÇÃO, POIS É FEITO POR TERRITÓRIO DIRETAMENTE NO ORACLE
*-------------------------*
User Function RJFP5RCA()
*-------------------------*  
Local aAreaTMP := GetArea()
Local nPOSARM := aScan( aHeader, {|x| AllTrim(Upper(x[2])) == "C6_LOCAL" })
Local cCODARM := aCols[n][nPOSARM]

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ
//³Chama função para monitor uso de fontes customizados³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ
U_USORWMAKE(ProcName(),FunName())

	If Type("M->C5_TIPO") != "U"
	
		If  M->C5_TIPO != "D"		
			If !Empty(M->C5_VEND1) .and. !Empty(M->C5_X_CLVL)
				dbSelectArea("Z22")
				dbSetOrder(1)
				dbGoTop()
				If dbSeek(xFilial("Z22") + M->C5_VEND1 + AllTrim(M->C5_X_CLVL))
					cCODARM := Z22->Z22_ARMAZ
				Else
					Aviso("Aviso","Não foi localizado relacionamento Armazém x Vendedor X Classe Valor para os dados deste pedido!", {"OK"}, 2)	
				EndIf
			Else
				Aviso("Aviso","Por favor informar os campos vendedor e classe de valor antes do código do produto!", {"OK"}, 2)			
			EndIf        		
		EndIf	
	
	EndIf                                  
	
	RestArea(aAreaTMP)                 

Return (cCODARM)
*/