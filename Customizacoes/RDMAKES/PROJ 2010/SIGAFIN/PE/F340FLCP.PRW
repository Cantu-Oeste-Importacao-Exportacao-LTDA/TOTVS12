*-----------------------------*
Static Function AjustaSX1()
*-----------------------------* 

aRegs  := {} 
aAdd(aRegs,{cPerg,"01","Natureza De     ?","Natureza De     ?","Natureza De     ?","mv_ch1","C",TAMSX3("E2_NATUREZ")[1],0,0,"G","","mv_par01","","","","","","","","","","","","","","","","","","","","","","","","", "SED","","",""})
aAdd(aRegs,{cPerg,"02","Natureza At้    ?","Natureza At้    ?","Natureza At้    ?","mv_ch2","C",TAMSX3("E2_NATUREZ")[1],0,0,"G","","mv_par02","","","","","","","","","","","","","","","","","","","","","","","","", "SED","","",""})
aAdd(aRegs,{cPerg,"03","Vencimento De   ?","Vencimento De   ?","Vencimento De   ?","mv_ch3","D",8,0,0,"G","","mv_par03","","","","","","","","","","","","","","","","","","","","","","","","", "","","",""})
aAdd(aRegs,{cPerg,"04","Vencimento At้  ?","Vencimento At้  ?","Vencimento At้  ?","mv_ch4","D",8,0,0,"G","","mv_par04","","","","","","","","","","","","","","","","","","","","","","","","", "","","",""})

	dbSelectArea("SX1")
	dbSetOrder(1) 
	
	For i := 1 To Len(aRegs)
		If !dbSeek(cPerg + aRegs[i,2])
			RecLock("SX1", .T.)
			For j := 1 to FCount()
				If j <= Len(aRegs[i])
					FieldPut(j, aRegs[i,j])	 
				Endif
			Next
			MsUnlock()
		Endif
	Next         

Return Nil