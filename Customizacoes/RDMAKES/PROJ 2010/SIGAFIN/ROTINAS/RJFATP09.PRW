*-------------------------*
User Function RJFP9VTP()
*-------------------------*
Local cAliasTMP := GetNextAlias()
Local lRet := .T.

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤ
//ณChama fun็ใo para monitor uso de fontes customizadosณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤ
U_USORWMAKE(ProcName(),FunName())

	If Empty(M->Z50_CODCD)
		
		Aviso("Aviso","C๓digo do CD deverแ ser informado antes da tabela de pre็o.",{"OK"})		
		lRet := .F.
		
	ElseIf !Empty(M->Z50_TABPRC)
		dbSelectArea("Z60")
		dbSetOrder(1)
		If dbSeek(xFilial("Z60")+M->Z50_CODCD)

			If Z60->Z60_CODEM == "31"
				cTABDA0 := "DA0300"
				cCODFIL := Space(TAMSX3("DA0_FILIAL")[1])
			Else 
				cTABDA0 := "DA0"+Z60->Z60_CODEM+"0"
				cCODFIL := Z60->Z60_CODFL
			EndIf
	
			cQuery := "		SELECT COUNT(*) NUMREG 						"
			cQuery += "		FROM "+cTABDA0+"							"
			cQuery += "		WHERE DA0_FILIAL 	= '"+cCODFIL+"'			"
			cQuery += "		AND DA0_CODTAB 		= '"+M->Z50_TABPRC+"' 	"
			cQuery += "		AND DA0_X_EB2B 		= 'S'				 	"
			cQuery += "		AND D_E_L_E_T_ 		<> '*' "				"
			
			TCQUERY ChangeQuery( cQuery ) NEW ALIAS (cAliasTMP)
			dbSelectArea(cAliasTMP)
			(cAliasTMP)->(dbGoTop())
			If (cAliasTMP)->(!EOF())
				If (cAliasTMP)->NUMREG == 0
					lRet := .F.
				EndIf
			EndIf
			(cAliasTMP)->(dbCloseArea()) 
			
			If ! lRet
				Aviso("Aviso","Tabela de pre็o "+M->Z50_TABPRC+" nใo localizada na empresa/filial referente ao CD ("+M->Z50_CODCD+") ou a tabela de pre็o nใo possui integra็ใo com B2B.",{"OK"})		
			EndIf
				
		EndIf
	EndIf
		

Return (lRet)


