Static Function fChkPerfil(cPERFIL)

Local cAliasTMP := GetNextAlias()
Local lRet := .T.

	cQuery := " SELECT COUNT(*) NUMREG FROM "+RetSQLName("SA1")+" WHERE A1_X_CPB2B = '"+cPERFIL+"' AND D_E_L_E_T_ <> '*' "
	
	TCQUERY ChangeQuery( cQuery ) NEW ALIAS (cAliasTMP)
	dbSelectArea(cAliasTMP)
	(cAliasTMP)->(dbGoTop())
	If (cAliasTMP)->(!EOF())
		If (cAliasTMP)->NUMREG > 0
			lRet := .F.
		EndIf
	EndIf
	(cAliasTMP)->(dbCloseArea())


Return (lRet)


