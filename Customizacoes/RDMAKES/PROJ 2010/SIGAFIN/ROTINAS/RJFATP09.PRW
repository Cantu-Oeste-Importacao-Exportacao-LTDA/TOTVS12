*-------------------------*
User Function RJFP9VPF()
*-------------------------* 
Local cAliasTMP := GetNextAlias()
Local lRet := .T.

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤ
//ณChama fun็ใo para monitor uso de fontes customizadosณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤ
U_USORWMAKE(ProcName(),FunName())

	If !Empty(M->A1_X_CPB2B)
		
		dbSelectArea("Z50")
		dbSetOrder(1)
		If dbSeek(xFilial("Z50")+M->A1_X_CPB2B)

			cQuery := "		SELECT COUNT(*) NUMREG 						"
			cQuery += "		FROM "+RetSQLName("Z61")+"					"
			cQuery += "		WHERE Z61_FILIAL 	= '"+xFilial("Z61")+"'	"
			cQuery += "		AND Z61_CODCD 		= '"+Z50->Z50_CODCD+"' 	"
			cQuery += "		AND Z61_CODUF 		= '"+M->A1_EST+"'	 	"
			cQuery += "		AND D_E_L_E_T_ 		<> '*' "				"
			
			TCQUERY ChangeQuery( cQuery ) NEW ALIAS (cAliasTMP)
			dbSelectArea(cAliasTMP)
			(cAliasTMP)->(dbGoTop())
			If (cAliasTMP)->(!EOF())
				If (cAliasTMP)->NUMREG == 0
					lRet := .F.
				EndIf
			EndIf
			(cAliasTMP)->(dbCloseArea()) 
			
			If ! lRet
				Aviso("Aviso","Este Perfil B2B estแ relacionado ao centro de distribui็ใo "+Z50->Z50_CODCD+" que nใo atende ao estado do cliente ("+M->A1_EST+").",{"OK"})		
			EndIf
		
		Else
			Aviso("Aviso","O Perfil B2B informado nใo foi localizado.",{"OK"})		
			lRet := .F.
		EndIf
				
	ElseIf Empty(M->A1_X_CPB2B) .and. M->A1_X_SB2B == "2"	//2=Aprovado
	
		Aviso("Aviso","Deverแ ser informado Perfil B2B pois o cliente possui o campo Status B2B = Aprovado.",{"OK"})		
		lRet := .F.
	
	EndIf

		

