User Function RJGPE08()
*-------------------------------*      
Private cPerg := "RJGPE00008" 

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤ
//ณChama fun็ใo para monitor uso de fontes customizadosณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤ
U_USORWMAKE(ProcName(),FunName())

	AjustaSX1()
	If Pergunte(cPerg,.T.)
	
		dbSelectArea("SRV")
		dbSetOrder(1)
		dbGoTop()
		While !SRV->(EOF())
			If !Empty(SRV->RV_MED13) .and. ALLTRIM(SRV->RV_MED13) != "N" .and. !Empty(mv_par01)
				RecLock("SRV",.F.)
				SRV->RV_MED13 := mv_par01
				SRV->(MsUnLock())
			EndIf
			If !Empty(SRV->RV_MEDFER) .and. ALLTRIM(SRV->RV_MEDFER) != "N" .and. !Empty(mv_par02)
				RecLock("SRV",.F.)
				SRV->RV_MEDFER := mv_par02
				SRV->(MsUnLock())
			EndIf
			If !Empty(SRV->RV_MEDAVI) .and. ALLTRIM(SRV->RV_MEDAVI) != "N" .and. !Empty(mv_par03)
				RecLock("SRV",.F.)
				SRV->RV_MEDAVI := mv_par03
				SRV->(MsUnLock())			
			EndIf
			SRV->(dbSkip())
		Enddo
		MsgInfo("Processo concluํdo!")
	
	EndIf


Return

