Static Function PRODGRV()

Local aCols		:= oBrw1:aCols
Local aHeader	:= oBrw1:aHeader

Do Case
	//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
	//Executa regras para a inclus„o do cadastro									≥
	//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
	Case INCLUI
		
		//Controle de transaÁ„o
		BEGIN TRANSACTION
			dbSelectArea("Z78")
			For nI := 1 to Len(aCols)
				If !aCols[nI][Len(aHeader) + 1]
					RecLock("Z78", .T.)
						//Passa campo a campo do aHeader para verificar se ele È real
						For nY := 1 to Len(aHeader)
							//Verifica se o campo nY È diferente de virtual, no caso se o campo È real
							If aHeader[nY][10] != "V"
								If !EMPTY(nPosZ78 := FieldPos(aHeader[nY][2]))
									Z78->(FieldPut(nPosZ78, aCols[nI][nY]))
								EndIf
							EndIf
						Next nY
						Z78->Z78_FILIAL	:= xFilial("Z78")
						Z78->Z78_DIA	:= dDia
					Z78->(MSUNLOCK())
				EndIf
			Next nI	
		
		END TRANSACTION
	
	
	//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
	//Executa regras para a alteraÁ„o do cadastro									≥
	//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹		
	Case ALTERA
	
		BEGIN TRANSACTION
		
			//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
			//Realiza a exclus„o de todos os registros existentes atualmente				≥
			//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
			dbSelectArea("Z78")
			Z78->(dbSetOrder(2)) // FILIAL + CODIGO
			Z78->(dbGoTop())
			If Z78->(dbSeek(xFilial("Z78") + DTOS(dDia)))
				While Z78->(!EOF()) .And. Z78->Z78_FILIAL == xFilial("Z78") .And. Z78->Z78_DIA == dDia
							
				//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
				//Efetua ‡ exclus„o do registro posicionado										≥
				//‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹‹
				RecLock("Z78", .F.)
					Z78->(DBDELETE())
				Z78->(MSUNLOCK())
			
				Z78->(dbSkip())
				End
			EndIf		
			
			dbSelectArea("Z78")
			For nI := 1 to Len(aCols)
				If !aCols[nI][Len(aHeader) + 1]
					RecLock("Z78", .T.)
						//Passa campo a campo do aHeader para verificar se ele È real
						For nY := 1 to Len(aHeader)
							//Verifica se o campo nY È diferente de virtual, no caso se o campo È real
							If aHeader[nY][10] != "V"
								If !EMPTY(nPosZ78 := FieldPos(aHeader[nY][2]))
									Z78->(FieldPut(nPosZ78, aCols[nI][nY]))
								EndIf
							EndIf
						Next nY
						
						Z78->Z78_FILIAL	:= xFilial("Z78")
						Z78->Z78_DIA	:= dDia
					Z78->(MSUNLOCK())
				EndIf
			Next nI	
					
		END TRANSACTION

	EndCase

Return