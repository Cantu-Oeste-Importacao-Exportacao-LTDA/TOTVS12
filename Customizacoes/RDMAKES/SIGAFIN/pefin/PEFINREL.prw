#INCLUDE "rwmake.ch"

User Function PEFINREL(_aErros)


Local cDesc1        := "Este programa tem como objetivo imprimir relatorio "
Local cDesc2        := "de acordo com os parametros informados pelo usuario."
Local cDesc3        := ""
Local cPict         := ""
Local titulo       	:= "Relatorio Retorno Serasa"
Local nLin         	:= 80
                   	
Local Cabec1		:= "Duplicata Cli/Loja  Nome cliente                      Observações"
Local Cabec2       	:= "                           Detalhes erro                         "
Local imprime      	:= .T.
Local aOrd 			:= {}
Private lEnd        := .F.
Private lAbortPrint := .F.
Private CbTxt       := ""
Private limite      := 80
Private tamanho     := "P"
Private nomeprog    := "PEFINREL" 
Private nTipo       := 18
Private aReturn     := { "Zebrado", 1, "Administracao", 2, 2, 1, "", 1}
Private nLastKey    := 0
Private cbtxt      	:= Space(10)
Private cbcont     	:= 00
Private CONTFL     	:= 01
Private m_pag      	:= 01
Private wnrel      	:= "PEFINREL" 

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ
//³Chama função para monitor uso de fontes customizados³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ
U_USORWMAKE(ProcName(),FunName())

wnrel := SetPrint("",NomeProg,"",@titulo,cDesc1,cDesc2,cDesc3,.F.,aOrd,.F.,Tamanho,,.F.)

If nLastKey == 27
	Return
Endif

SetDefault(aReturn,"")

If nLastKey == 27
   Return
Endif

nTipo := If(aReturn[4]==1,15,18)

RptStatus({|| RunReport(Cabec1,Cabec2,Titulo,nLin) },Titulo)
Return


Static Function RunReport(Cabec1,Cabec2,Titulo,nLin)

Local nOrdem



For nR := 1 To Len(_aErros)

    If lAbortPrint
       @nLin,00 PSAY "*** CANCELADO PELO OPERADOR ***"
       Exit
    Endif

    If nLin > 60
       Cabec(Titulo,Cabec1,Cabec2,NomeProg,Tamanho,nTipo)
       nLin := 9
    Endif

//	_cTitulo,_cParc,_cCli,_cLoja,_cErros,_TpMov
	@nLin, 000 Psay _aErros[nR,1]
	@nLin, 007 Psay _aErros[nR,2]
	@nLin, 010 Psay _aErros[nR,3]
	@nLin, 017 Psay _aErros[nR,4]
	DbSelectArea("SA1")
	SA1->(DbGotop())
	SA1->(DbSetOrder(1))
	DbSeek(xFilial("SA1")+_aErros[nR,3]+_aErros[nR,4])
	@nLin, 030 Psay SubStr(SA1->A1_NOME,1,25)
	If AllTrim(_aErros[nR,5])	== "INCLUSAO OK" .OR. AllTrim(_aErros[nR,5]) == "EXCLUSAO OK"
		@nLin, 057 Psay _aErros[nR,5]
	Else
		@nLin, 057 Psay "OS ERROS ABAIXO FORAM ENCONTRADOS"
		nLin += 1
	    If nLin > 60
	       Cabec(Titulo,Cabec1,Cabec2,NomeProg,Tamanho,nTipo)
	       nLin := 9
	    Endif		
		nErr	:= 1
		For nX := 1 To (Len(AllTrim(_aErros[nR,5]))/3)
			If nX = 1
				@nLin, 030 Psay SubStr(_aErros[nR,5],nX,3)
				@nLin, 034 Psay AllTrim(PefinErr(SubStr(_aErros[nR,5],nX,3)))
			Else
				@nLin, 020 Psay SubStr(_aErros[nR,5],nErr,3)			
				@nLin, 024 Psay AllTrim(PefinErr(SubStr(_aErros[nR,5],nErr,3)))				
			Endif
			nErr += 3
			nLin += 1
		    If nLin > 60
		       Cabec(Titulo,Cabec1,Cabec2,NomeProg,Tamanho,nTipo)
		       nLin := 9
		    Endif						
		Next nX 
	Endif
	nLin += 1

Next nR

SET DEVICE TO SCREEN

If aReturn[5]==1
   dbCommitAll()
   SET PRINTER TO
   OurSpool(wnrel)
Endif

MS_FLUSH()

Return

