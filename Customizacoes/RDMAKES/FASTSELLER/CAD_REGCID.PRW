					 { OemToAnsi("Visualizar") 		,"U_MANUTCAD" , 0 , 2} ,;
					 { OemToAnsi("Incluir")    		,"U_MANUTCAD" , 0 , 3} ,;
					 { OemToAnsi("Alterar")    		,"U_MANUTCAD" , 0 , 4} ,;
